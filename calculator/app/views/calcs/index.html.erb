<p id="notice"><%= notice %></p>

<h1>Listing Calcs</h1>

<table>
  <thead>
    <tr>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calcs.each do |calc| %>
      <tr>
        <td><%= calc.result %></td>
        <td><%= link_to 'Show', calc %></td>
        <td><%= link_to 'Edit', edit_calc_path(calc) %></td>
        <td><%= link_to 'Destroy', calc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<table>
  <thead>
  <tr>
    <th>try</th>
      </tr>
    </thead>
    <tbody>

   <%= form_tag('/calcs') do %>
    <% @numbers.each do |n| %>
    <td><%= submit_tag n, type: "button" %></td>
    
    <% end %>
   <% end %> 

  

  </tbody>
</table>
